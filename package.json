{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend is the ultimate backend solution",
  "main": "src/server.js",
  "scripts": {
    "setup": "npm install && npm update",
    "tsc": "tsc",
    "build": "npm run tsc",
    "start": "ts-node-dev --transpile-only ./src/server.ts",
    "dev": "ts-node-dev --respawn --transpile-only ./src/server.ts",
    "prod": "tsc && node ./build/src/server.js",
    "prepare": "husky install",
    "lint": "eslint --fix"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "keywords": [
    "express",
    "backend",
    "backend"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@types/express": "^4.17.1",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.5.0",
    "express-custom-error": "^1.8.4",
    "has-keys": "^0.1.0",
    "helmet": "^3.21.2",
    "http": "^0.0.1-security",
    "http-status": "^1.3.2",
    "husky": "^9.0.11",
    "joi": "^17.13.3",
    "joi-password-complexity": "^5.2.0",
    "jsonwebtoken": "^9.0.2",
    "mandatoryenv": "^1.1.1",
    "module-alias": "^2.2.2",
    "mongodb": "^3.3.3",
    "mongoose": "^8.4.4",
    "morgan": "^1.9.1",
    "prettier": "^3.3.2",
    "rotating-file-stream": "^1.4.6",
    "socket-io": "^1.0.0",
    "socket.io": "^4.7.5",
    "ts-node-dev": "^1.0.0-pre.43",
    "typescript": "^5.5.2"
  },
  "_moduleAliases": {
    "@root": "src",
    "@controllers": "src/controllers",
    "@models": "src/models",
    "@util": "src/util",
    "@routes": "src/routes"
  }
}
